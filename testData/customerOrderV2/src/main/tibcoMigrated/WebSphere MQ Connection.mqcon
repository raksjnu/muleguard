<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>WebSphere MQ Connection</name>
    <resourceType>plugin.bwmq.ConnectionResource</resourceType>
    <config>
        <version>7.05.00</version>
        <qmanagerbindingmode>remote</qmanagerbindingmode>
        <host>localhost</host>
        <qmanagerport>1414</qmanagerport>
        <qmanager/>
        <pooled>false</pooled>
        <qmanageractive>9</qmanageractive>
        <qmanagerunused>3</qmanagerunused>
        <qmanagertimeout>100</qmanagertimeout>
        <tls>false</tls>
        <keyorident>Keystore</keyorident>
        <hdrcomp>false</hdrcomp>
        <msgcomp>msgcomp_none</msgcomp>
        <serverchannel>CITI.POC.CHANNEL</serverchannel>
        <userid>test</userid>
        <PASSWORD>test</PASSWORD>
    </config>
</BWSharedResource>